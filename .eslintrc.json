{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier":
    [ "error", {
      "endOfLine":"auto",
      "printWidth": 119,
      "trailingComma": "es6",
      "semi": true,
      "doubleQuote": true,
      "jsxSingleQuote": true,
      "singleQuote": false,
      "useTabs": true,
      "tabWidth": 4
    }]
  }
}